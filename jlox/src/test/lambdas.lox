fun caller(fn) {
    fn();
}

fun caller_2(fn, value) {
    fn(value);
}

caller(fun() {
    print "lambda!";
});

caller_2(fun(v){
    print "lambda " + v + "!";
}, "hello");

var a = fun() {
    print "called lambda assigned to var";
};

print a;
a();

caller(a);

fun thrice(fn) {
    for (var i = 1; i <= 3; i = i+1) {
        fn(i);
    }
}

fun printer(i) {
    print "printer " + i;
}

thrice(printer);

thrice(fun(i) {
    print "lambda! " + i;
});
